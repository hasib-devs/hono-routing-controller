{
  "name": "hono-routing-controller",
  "version": "0.1.1",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup src/index.ts --watch",
    "test": "tsx --test tests/**/*.spec.ts",
    "test:watch": "tsx --test --watch tests/**/*.spec.ts",
    "test:coverage": "tsx --test --experimental-test-coverage tests/**/*.spec.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "hono",
    "routing",
    "controller",
    "typescript",
    "middleware",
    "dependency-injection"
  ],
  "author": "Hasibur Rahman",
  "license": "MIT",
  "description": "A routing controller package for Hono.js",
  "peerDependencies": {
    "hono": "^4.7.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/core": "^1.11.5",
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.22.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.0.0",
    "semantic-release": "^24.2.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "repository": {
    "url": "git://github.com/hasib-devs/hono-routing-controller.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
