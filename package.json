{
  "name": "hono-routing-controller",
  "version": "0.0.3",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --minify --sourcemap",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch --sourcemap",
    "test": "node --loader ts-node/esm --test",
    "test:watch": "node --loader ts-node/esm --test --watch",
    "test:coverage": "node --experimental-test-coverage --loader ts-node/esm --test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "hono",
    "routing",
    "controller",
    "typescript",
    "middleware",
    "dependency-injection"
  ],
  "author": "Hasibur Rahman",
  "license": "MIT",
  "description": "A routing controller package for Hono.js",
  "dependencies": {
    "hono": "^4.7.2",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "hono": "^4.7.2",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@swc/core": "^1.11.11",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "semantic-release": "^24.2.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "repository": {
    "url": "git://github.com/hasib-devs/hono-routing-controller.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
